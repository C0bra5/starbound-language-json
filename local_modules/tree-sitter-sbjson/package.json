{
  "name": "tree-sitter-sbjson",
  "version": "0.0.1",
  "description": "Starbound-JSON grammar for tree-sitter. This is a modified version of tree-sitter-json that contains support for multi-line comments and extra identifiers to help better highlight the text.",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "sbjson",
    "starbound"
  ],
  "author": "Guillaume Mercier",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.0.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.15.14"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.sbjson",
      "file-types": [
        "2functions",
        "achievement",
        "activeitem",
        "aimission",
        "animation",
        "augment",
        "back",
        "beamaxe",
        "behavior",
        "behavior-project",
        "biome",
        "bush",
        "chest",
        "cinematic",
        "codex",
        "collection",
        "combofinisher",
        "config",
        "configfunctions",
        "consumable",
        "currency",
        "cursor",
        "damage",
        "disabled",
        "dungeon",
        "effectsource",
        "event",
        "flashlight",
        "frames",
        "functions",
        "grass",
        "harvestingtool",
        "head",
        "inspectiontool",
        "instrument",
        "item",
        "itemdescription",
        "legs",
        "liqitem",
        "liquid",
        "material",
        "matitem",
        "matmod",
        "miningtool",
        "modularfoliage",
        "modularstem",
        "monsterpart",
        "monsterskill",
        "monstertype",
        "namesource",
        "nodes",
        "npctype",
        "object",
        "painttool",
        "parallax",
        "particle",
        "particlesource",
        "partparams",
        "patch",
        "projectile",
        "questtemplate",
        "radiomessage",
        "recipe",
        "spawntypes",
        "species",
        "stagehand",
        "statuseffect",
        "structure",
        "tech",
        "tenant",
        "terrain",
        "thrownitem",
        "tillingtool",
        "tooltip",
        "treasurepools",
        "unlock",
        "vehicle",
        "weaponability",
        "weather",
        "wiretool"
      ]
    }
  ]
}
